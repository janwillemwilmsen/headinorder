{
	"configurations": [
		{
			"name": "Cloud Run: Run/Debug Locally",
			"type": "cloudcode.cloudrun",
			"request": "launch",
			"build": {
				"docker": {
					"path": "Dockerfile"
				}
			},
			"image": "getheadone",
			"service": {
				"name": "getheadone",
				"containerPort": 8080,
				"resources": {
					"limits": {
						"cpu": 2,
						"memory": "2048Mi"
					}
				}
			},
			"target": {
				"minikube": {}
			},
			"watch": true
		}
	]
}